{
        "layer": "top",
        "position": "top",
        "mod": "dock",
        "height": 28,
           "margin-left": 5,
           "margin-right": 5,
           "margin-top": 3,
           "margin-bottom": 0,
        "exclusive": true,
        "passthrough": false,
        "gtk-layer-shell": true,
        "reload_style_on_change": true,
    
    	  "modules-left": ["cpu","memory", "custom/storage", "custom/updates","tray"],
 
        "modules-center": ["hyprland/workspaces","hyprland/window"],
	
        "modules-right": ["network","backlight","pulseaudio","clock","battery","custom/power"],
    
        "cpu": {
            "interval": 10,
            "format": "<span color='#f5e0dc'>󰍛</span> {usage}%",
            "rotate": 0,
            "format-alt": "{icon0}{icon1}{icon2}{icon3}",
            "format-icons": ["▁", "▂", "▃", "▄", "▅", "▆", "▇", "█"],
        },
    
        "memory": {
            "states": {
                "c": 90, // critical
                "h": 60, // high
                "m": 30 // medium
            },
            "interval": 30,
            "format": "<span color='#f5c2e7'>󰾆</span> {used}GB",
            "rotate": 0,
            "format-m": "󰾅 {used}GB",
            "format-h": "󰓅 {used}GB",
            "format-c": " {used}GB",
            "format-alt": "󰾆 {percentage}%",
            "max-length": 10,
            "tooltip": true,
            "tooltip-format": "󰾆 {percentage}%\n {used:0.1f}GB / {total:0.1f}GB",
        },
             
        "custom/storage": {
            "exec": "~/.config/waybar/modules/storage.sh",
            "return-type": "json",
            "interval": 30,
            "format": "<span color='#f9e2af'>󰋊</span> {}",
            "tooltip": true,
            "on-click": "thunar /",
            "on-click-right": "gnome-disk-utility"
        },
  
        "custom/updates": {
            "exec": "~/.config/waybar/modules/system-update.sh",
            "return-type": "json",
            "format": "{}",
            "on-click": "~/.config/waybar/modules/system-update.sh update",
            "interval": 86400,
            "tooltip": true,
            "signal": 20
        },

        "tray": {
            "icon-size": 15,
            "spacing": 0
        },

        "hyprland/workspaces": {
            "disable-scroll": true,
            "rotate": 0,
            "all-outputs": true,
            "active-only": false,
            "on-click": "activate",
            "disable-scroll": false,
            "on-scroll-up": "hyprctl dispatch workspace -1",
            "on-scroll-down": "hyprctl dispatch workspace +1",
            "persistent-workspaces": {
            }
        },
    
        "hyprland/window": {
            "format": " {}",
            "rotate": 0,
            "separate-outputs": true,
            "rewrite": {
                "fish": "Terminal 󰆍",
                "kitty": "Terminal 󰆍",
                "(.*) - fish": "$1 󰆍",
                "romanhan@archlinux:(.*)": "$1 ",
                
                "(.*) — Mozilla Firefox": "$1 󰈹",
                "(.*)Mozilla Firefox": "Firefox 󰈹",
                "(.*) — Google chrome": "$1 ",
                "(.*)Google chrome": "Chrome ",
                "(.*) - Chromium": "$1 ",
                "(.*)Chromium": "Chromium ",
                "(.*) — Zen Browser": "$1 󰈹",
                "(.*)Zen Browser": "Zen 󰈹",
                "(.*) - Brave": "$1 ",
                "(.*)Brave": "Brave ",

                "(.*) - Visual Studio Code": "$1 󰨞",
                "(.*)Visual Studio Code": "Code 󰨞",
                "(.*) - Code - OSS": "$1 󰨞",
                "(.*)Code - OSS": "Code 󰨞",

                "(.*) Thunar": "$1 󰉋",

                "(.*)Spotify": "Spotify 󰓇",
                "(.*)Spotify Free": "Spotify 󰓇",
                "(.*)Spotify Premiun": "Spotify 󰓇",

                "(.*)Steam": "Steam 󰓓",
                "(.*) - Discord": "$1 ",
                "(.*)Netflix": "Netflix 󰝆",
                
                "(.*) — Github": "$1 ",
                "(.*)Github": "Github "
            },
            "max-length": 70
        },

        "network": {
            "tooltip": true,
            "format-wifi": "",
            "rotate": 0,
            "format-ethernet": "󰈀",
            "tooltip-format": "Network: <big><b>{essid}</b></big>\nSignal strength: <b>{signaldBm}dBm ({signalStrength}%)</b>\nFrequency: <b>{frequency}MHz</b>\nInterface: <b>{ifname}</b>\nIP: <b>{ipaddr}/{cidr}</b>\nGateway: <b>{gwaddr}</b>\nNetmask: <b>{netmask}</b>",
            "format-linked": "󰈀 {ifname} (No IP)",
            "format-disconnected": "󰖪",
            "tooltip-format-disconnected": "Disconnected",
            "format-alt": "<span foreground='#99ffdd'>  {bandwidthDownBytes}</span>  <span foreground='#ffcc66'>  {bandwidthUpBytes}</span>",
            "interval": 2
        },
        
        "backlight": {
            "device": "intel_backlight",
            "rotate": 0,
            "format": "<span color='#eba0ac'>{icon}</span> {percent}%",
            "format-icons": ["", "", "", "", "", "", "", "", ""],
            "tooltip-format": "Brightness {percent}% ",
            "on-scroll-up": "brightnesscontrol.sh i 1",
            "on-scroll-down": "brightnesscontrol.sh d 1",
            "min-length": 2
        },
    
        "pulseaudio": {
            "format": "<span color='#f9e2af'>{icon}</span> {volume}",
            "rotate": 0,
            "format-muted": " ",
            "on-click": "pavucontrol -t 3",
            "on-click-right": "volumecontrol.sh -s ''",
            "on-click-middle": "volumecontrol.sh -o m",
            "on-scroll-up": "volumecontrol.sh -o i",
            "on-scroll-down": "volumecontrol.sh -o d",
            "tooltip-format": "{icon} {desc} // {volume}%",
            "scroll-step": 5,
            "format-icons": {
                "headphone": "",
                "hands-free": "",
                "headset": "",
                "phone": "",
                "portable": "",
                "car": "",
                "default": ["", "", ""]
            }
         },

         "clock": {
            "format": "<span color='#a6e3a1'>󰥔</span> {:%H:%M}",
            "format-alt": "<span color='#a6e3a1'>󰥔</span> {:%H:%M <span color='#94e2d5'>󰃮</span> %d.%m.%y}",
            "rotate": 0,
            "tooltip-format": "<span>{calendar}</span>",
            "calendar": {
                "mode": "month",
                "mode-mon-col": 3,
                "on-scroll": 1,
                "on-click-right": "mode",
                "format": {
                    "months": "<span color='#fab387'><b>{}</b></span>",
                    "weekdays": "<span color='#a6e3a1'><b>{}</b></span>",
                    "today": "<span color='#cba6f7'><b>{}</b></span>"
                }
            },
            "actions": {
                "on-click-right": "mode",
                "on-click-forward": "tz_up",
                "on-click-backward": "tz_down",
                "on-scroll-up": "shift_up",
                "on-scroll-down": "shift_down"
            }
          },
         
        "battery": {
            "states": {
                "good": 95,
                "warning": 30,
                "critical": 20,
                "verylow": 10            },
            "format": "{icon} {capacity}%",
            "rotate": 0,
            "format-charging": " {capacity}%",
            "format-plugged": " {capacity}%",
            "format-alt": "{time} {icon}",
            "format-icons": ["󰂎", "󰁺", "󰁻", "󰁼", "󰁽", "󰁾", "󰁿", "󰂀", "󰂁", "󰂂", "󰁹"]
        },
    
        "custom/power": {
            "format": "{}",
            "rotate": 0,
            "exec": "echo ; echo  logout",
            "on-click": "~/.config/rofi/powermenu/powermenu.sh",
            "interval" : 86400, // once every day
            "tooltip": true
        }
    
    }
